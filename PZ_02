package com.metanit;

public class Main {

    public static void main(String[] args) {

        int i = 35;
        String str = Integer.toString(i);
        System.out.println(str);
        double  i2 = 32.4e10;
        String str2 = Double.toString(i2);
        System.out.println(str2);
        long  i3 = 3422222;
        String str3 = Long.toString(i3);
        System.out.println(str3);
        float  i4 = 3.46f;
        String str4 = Float.toString(i4);
        System.out.println(str4);


    }
}
//
package com.metanit;

public class Main {

    public static void main(String[] args) {
        char ch = 'S';

        // c использованием класса Character
        String charToString = Character.toString(ch);
        System.out.println(charToString);

        // с использованием операции добавления класса String
        String str = "" + ch;
        System.out.println(str);

        //с использованием массива
        String fromChar = new String(new char[] { ch });
        System.out.println(fromChar);

        // с использованием метода valueOf класса String
        String valueOfchar = String.valueOf(ch);
        System.out.println(valueOfchar);


        //char to int
        char ch2 = '9';

        // c использованием метода getNumericValue
        // класса Character
        int i1 = Character.getNumericValue(ch2);
        System.out.println(i1);

        // c использованием метода digit класса Character
        int i2 = Character.digit(ch2,10);
        System.out.println(i2);




    }
}
//
package com.metanit;

public class Main {

    public static void main(String[] args) {
        //int to long
        int i = 2015;
        long l = (long) (i);
        System.out.println(l);

        //int to float
        int i2 = 2015;
        float f = (float) (i2);
        System.out.println(f);

       // long to int
        long l3 = 214748364;
        int i3 = (int) l3;
        System.out.println(i3);

       // double to int
        double d = 3.14;
        int i4 = (int) d;
        System.out.println(i4);

    }
}

//Материал из лекции
package com.metanit;

import java.awt.*;

public class Main {

    public static void main(String[] args) {

        Point p1 = new Point(3,5);
        Point p2=p1;
        p1 = new Point(7,9);
        System.out.println(p2.x);
    }
}
